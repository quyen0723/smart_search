[run]
source = src/smart_search
branch = true
parallel = true
omit =
    */tests/*
    */__pycache__/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if TYPE_CHECKING:
    if __name__ == .__main__.:
    @abstractmethod
    @overload

fail_under = 85
show_missing = true
precision = 2

[html]
directory = htmlcov
